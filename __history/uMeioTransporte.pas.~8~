unit uMeioTransporte;

interface

uses Dialogs;

type

TMeioTransporte = class
  Modelo : string;
  Marca : string;
  Ano : integer;
  Preco : integer;
  procedure Cadastro; virtual;
end;

implementation

{ TMeioTransporte }

procedure TMeioTransporte.Cadastro;
begin
  ShowMessage('Cadastro Efetuado do seu' + Modelo);
end;

end.
